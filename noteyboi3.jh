
application {
  config {
    baseName Notey,
    applicationType monolith,
    packageName net.kooksdoes,
    authenticationType oauth2,
    prodDatabaseType postgresql,
    clientFramework react
  }
  entities *
}


entity Note {
 Id Integer
 Content String
 Title String
 Xpos Double
 Ypos Double
}

entity Tag {
Id Integer
Name String
}

relationship ManyToMany {
Note to Tag
}

relationship ManyToOne{
Tag to User
}
    
relationship ManyToOne{
Tag to User
}